{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": true,
  "printWidth": 150,
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "useTabs": false,
  "tabWidth": 2,
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": false,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-css-order", "prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
  "attributeGroups": [
    "^((v-bind)?:?|v-)is$",
    "^v-for$",
    "^v-(if|else-if|else|show|cloak)$",
    "^v-(once|pre|memo)$",
    "^(v-bind)?:?id$",
    "^(v-bind)?:?key$",
    "^(v-bind)?:?ref$",
    "^(v-)?slot$",
    "^#",
    "^v-model$",
    "^v-(?!bind(:|$)|on(:|$)|html$|text$)",
    "^class$",
    "^(v-bind)?:class$",
    "^((v-bind)?:)?(?!data-|v-|:|@|#)",
    "$DEFAULT",
    "^((v-bind)?:)?data-",
    "^v-bind$",
    "^v-on:",
    "^@",
    "^v-html$",
    "^v-text$"
  ]
}
